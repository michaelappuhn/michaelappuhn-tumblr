
{block:Photo}
	<figure class="post--photo__image {block:HighRes}high-res{/block:HighRes}{block:Caption} with-caption{/block:Caption}" data-photo-width="{PhotoWidth-HighRes}">
	  {LinkOpenTag}
		  <img src="{PhotoURL-HighRes}" alt="{PhotoAlt}" width="{PhotoWidth-HighRes}" height="{PhotoHeight-HighRes}">
	  {LinkCloseTag}
	  {block:Caption}
		  <figcaption class="caption photo__caption">{Caption}</figcaption>
	  {/block:Caption}
	</figure>
{/block:Photo}
